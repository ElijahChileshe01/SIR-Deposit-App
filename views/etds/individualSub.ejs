        text-align: left;
    }
    .buttonB{
        border-radius: 8px;
    }
</style>

<%- include ("../partials/navigation") %><br><br>

    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
